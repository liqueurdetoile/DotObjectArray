{
  "name": "dot-object-array",
  "version": "0.0.0",
  "description": "Implements array-like methods for Object with support for dot notation keys",
  "main": "dist/objectarray.min.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "webpack --config webpack.conf/build.js",
    "dev": "webpack --progress --colors --watch --config webpack.conf/dev.js",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:dry": "./node_modules/.bin/karma start --single-run --browsers ChromeHeadless",
    "test:dev": "karma start",
    "docs": "./node_modules/.bin/esdoc",
    "prepub": "npm run test:dry && npm run build && npm run docs"
  },
  "devDependencies": {
    "ajv": "^6.9.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "color-themes-for-google-code-prettify": "^2.0.4",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "docdash": "^1.1.0",
    "esdoc": "^1.1.0",
    "esdoc-inject-script-plugin": "^1.0.0",
    "esdoc-inject-style-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.0.1",
    "karma-babel-preprocessor": "^8.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.1.2",
    "phantomjs-prebuilt": "^2.1.16",
    "semantic-release": "^15.13.4",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liqueurdetoile/dotobjectarray.git"
  },
  "keywords": [
    "object",
    "array",
    "array-like",
    "dot notation"
  ],
  "author": "Liqueur de Toile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liqueurdetoile/dotobjectarray/issues"
  },
  "homepage": "https://github.com/liqueurdetoile/dotobjectarray",
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
